
<main class="main">
   <div class="search-container">
      <form id="search-form">
         <input type="search" name="search-input" placeholder="search meal"id='search-input' >
         <button type="submit">Search</button>
      </form> 
   </div> 
<div class="new-header">
   <h2>Our Freshly Added</h2>
</div> 
<div class="contenerek">
<% arrayfile.forEach(function(file) { %>
   <div class="new-added">
      <div class="new-content">
         <div class="header">
            <h3 id='dish-name1'><%=file.header%></h3>
         </div>
      
         <div class="image-content" id='new-image'>
            <img 
            id='new-image-content'
            src="data:image/<%=file.images.contentType%>;base64,
            <%=file.images.data.toString('base64')%>"
            alt="photo"
         />
         </div>

         <div class="button">
            <a href="" class="btn-verify">
               <button type="submit" id='<%=file._id%>''>more</button>
            </a>
        </div>
      </div>
   </div> 
   <% }) %> 
</div>
   <div class="btn-more">
      <a href="/newly-added">
      <button type="submit" class="more">See more</button>
   </a>
   </div>  
</main>

